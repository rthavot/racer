<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="net.sf.orcc.backends.backends">
      <backend
            class="ch.epfl.stimm.yace.backend.osci.OsciBackendImpl"
            name="OSCI">
         <option
               id="net.sf.orcc.plugins.compileXDF">
         </option>
         <option
               id="ch.epfl.stimm.yace.backend.osci.ui.omapping">
         </option>
      </backend>
   </extension>
   <extension
         id="osci"
         point="net.sf.orcc.core.options">
      <option
            id="ch.epfl.stimm.yace.backend.osci.ui.omapping"
            name="Mapping">
         <checkBox>
            <option
                  id="ch.epfl.stimm.yace.backend.osci.ui.mapping"
                  name="File">
               <browseFile
                     folder="false"
                     workspace="true">
               </browseFile>
            </option></checkBox>
      </option>
   </extension>
   <extension
         id="osci"
         name="osci"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
      </application>
   </extension>
</plugin>
